{
    "name": "IP Blocker",
    "description": "Blocks IPs",
    "manifest_version": 3,
    "version": "0.1.0",
    "options_page": "options.html",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "declarativeNetRequest",
      "declarativeNetRequestFeedback",
      "storage"
    ],
    "action": {
      "default_title": "My YT Bookmarks",
      "default_popup": "popup.html"
    }

  }